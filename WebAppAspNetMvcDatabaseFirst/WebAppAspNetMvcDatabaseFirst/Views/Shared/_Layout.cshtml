<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Госуслуги</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar-collapse navbar-light" style="background-color: #e3f2fd;">
        <div class="container">

            <nav class="navbar-collapse navbar-light" style="background-color: #e3f2fd;">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Клиенты", "Index", "Clients")</li>
                </ul>
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Услуги", "Index", "Orders")</li>
                </ul>
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Типы клиентов", "Index", "ClientTypes")</li>
                </ul>
            </nav>

        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

